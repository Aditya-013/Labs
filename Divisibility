#include<stdio.h>
void main()
 {
   //Testing for divisibility with three
   int n;
   int i;
   int sum;


   scanf("%d",&n);                //Logic one
   if(n%3==0)                  
     printf("It is divisible!");


   scanf("%d",&n);               //Logic two
   while(n<3)                  
    {
      n = n-3;
    }
   if(n==0)
    printf("It is divisible by!");


   scanf("%d",&n);              //Logic Three
   while(n!=0)                 
    {
      sum+=(n%10);
      n=n/10;
    }
  if(sum%3==0)
    printf("It is divisible");

   scanf("%d",&n);             //Logic four
   n=n*2;
    if(n%6==0) 
     printf("It is divisible");
  

   //Divisibility test for four
   scanf("%d",&n);           //Logic one
     if(n%4==0)
       printf("It is divisible");

   scanf("%d",&n);           //Logic two
   i=n%100;
   if(i%4==0)
     printf("It is divisible!");

   scanf("%d",&n);           //Logic three
     n=n*2;
   if(n%8==0)
     printf("It is divisible");
 }
